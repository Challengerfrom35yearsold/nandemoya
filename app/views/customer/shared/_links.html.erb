<%- if controller_name != 'sessions' %>
  <%= link_to "ログインする", new_session_path(resource_name), class:"btn btn-outline-secondary btn-block" %><br />
<% end %>

<%- if devise_mapping.registerable? && controller_name != 'registrations' %>
  <%= link_to "アカウントを作成する", new_registration_path(resource_name), class:"btn btn-outline-secondary btn-block" %><br />
<% end %>